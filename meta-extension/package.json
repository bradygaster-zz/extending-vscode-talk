{
    "name": "extending-vs-code-session",
    "displayName": "Session Demo for Extending Visual Studio Code",
    "description": "",
    "version": "0.0.1",
    "publisher": "bradygaster",
    "engines": {
        "vscode": "^1.10.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "*"
    ],
    "main": "./extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.sayHello",
                "title": "Hello World"
            },
            {
                "command": "meta.slowProcess",
                "title": "Run Slow Process"
            }
        ],
        "snippets": [
            {
                "language": "javascript",
                "path": "./snippets/console.json"
            }
        ]
    },
    "scripts": {
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.0.3",
        "vscode": "^1.0.0",
        "mocha": "^2.3.3",
        "eslint": "^3.6.0",
        "@types/node": "^6.0.40",
        "@types/mocha": "^2.2.32"
    }
}